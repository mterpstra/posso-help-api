      },
      {
        "start_day": 7,
        "end_day": 7,
        "event": "Second Dose of Medication",
        "treatments": [
          {
            "medication": "5,0 mL LUTALYSE"
          },
          {
            "notes": "If using 2-CIDR, this ends the first CIDR"
          }
        ]
      },
      {
        "start_day": 9,
        "end_day": 9,
        "event": "Third Dose of meciation",
        "treatments": [
          {
            "medication": "2,5 mL LUTALYSE"
          },
          {
            "medication": "+ 0,5 mL E.C.P."
          },
          {
            "notes": "End of CIDR"
          }
        ]
      },
      {
        "start_day": 11,
        "end_day": 11,
        "event": "IATF"
      }
    ],
    "account": "000000000000000000000000"
