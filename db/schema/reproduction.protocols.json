[
  {
    "_id": {
      "$oid": "6957249c86fed12ada04e50b"
    },
    "name": "7-day CO-Synch + CIDR",
    "description": "A common estrus synchronization and fixed-time artificial inseination (TAI) protocol for beef cattle.",
    "notes": "Cows typically have TAI later than heifers. This protocol requires only three trips through the chute.",
    "target_animals": [
      "Cows",
      "Heifers"
    ],
    "timeline_days": [
      {
        "start_day": 0,
        "end_day": 0,
        "event": "Initial administration of hormones and CIDR insertion",
        "treatments": [
          {
            "medication": "GnRH (gonadotropin-releasing hormone)",
            "notes": "e.g., Fertagyl, Cystorelin"
          },
          {
            "device": "CIDR (Controlled Internal Drug Release) insert",
            "notes": "Progesterone insert remains in place for 7 days"
          }
        ]
      },
      {
        "start_day": 7,
        "end_day": 7,
        "event": "CIDR removal and administration of Prostaglandin (PG)",
        "treatments": [
          {
            "device": "CIDR removal",
            "notes": "Remove intravaginal insert"
          },
          {
            "medication": "PGF2a (prostaglandin F2 alpha)",
            "notes": "e.g., Estrumate, Lutalyse, InSynch"
          }
        ]
      },
      {
        "start_day": 9,
        "end_day": 10,
        "event": "Fixed-time AI (TAI) with concurrent GnRH administration",
        "timing_cows": "60 to 66 hours after PGF2a injection/CIDR removal",
        "timing_heifers": "52 to 56 hours after PGF2a injection/CIDR removal",
        "treatments": [
          {
            "procedure": "Artificial Insemination (AI)",
            "notes": "Fixed-time AI (TAI)"
          },
          {
            "medication": "GnRH",
            "notes": "Administered at the time of AI"
          }
        ]
      }
    ],
    "account": "000000000000000000000000"
  },
